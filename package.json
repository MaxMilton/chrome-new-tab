{
  "name": "new-tab",
  "version": "0.5.0",
  "description": "A high performance new tab page that gets you where you need to go faster.",
  "author": "Max Milton <max@wearegenki.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "google-chrome",
    "ntp",
    "chrome-extension",
    "chrome",
    "marko",
    "markojs",
    "lasso",
    "maxmilton"
  ],
  "scripts": {
    "start": "yarn dev",
    "dev": "NODE_ENV=development node build/watch.js",
    "prebuild": "rm -rf .cache dist && mkdir -p dist/src",
    "build": "APP_RELEASE=$(git describe --always --dirty=\"-dev\") NODE_ENV=production node build/build.js",
    "FIXME__lint:css": "stylelint --cache --cache-location=/tmp/ \"**/*.{marko,html,css}\"",
    "lint:css": "stylelint --cache --cache-location=/tmp/ \"**/*.{html,css}\"",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.marko src test",
    "lint": "yarn lint:css && yarn lint:js",
    "test": "jest",
    "test-ci": "yarn test --coverage --runInBand --ci",
    "pretest:watch": "yarn lint",
    "test:watch": "yarn test --coverage --notify --watch"
  },
  "dependencies": {
    "marko": "4.9.4",
    "marko-bind": "0.1.0",
    "raven-js": "3.24.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.63",
    "@wearegenki/eslint-config": "0.7.0",
    "@wearegenki/postcss-config": "0.7.0",
    "@wearegenki/stylelint-config": "0.7.0",
    "@wearegenki/test": "0.8.0",
    "browser-sync": "2.23.6",
    "clean-css": "4.1.11",
    "codecov": "3.0.0",
    "eslint": "4.19.1",
    "jest": "22.4.3",
    "lasso": "3.1.4",
    "lasso-marko": "2.4.0",
    "lasso-postcss": "0.4.0",
    "optimize-js": "1.0.3",
    "postcss": "6.0.21",
    "puppeteer": "1.2.0",
    "stylelint": "9.1.3",
    "uglify-es": "3.3.10",
    "worker-nodes": "1.6.0"
  },
  "engines": {
    "node": ">= 6.9.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
