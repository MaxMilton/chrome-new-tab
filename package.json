{
  "private": true,
  "name": "new-tab",
  "version": "0.13.0",
  "description": "âš¡ A high performance new tab page that gets you where you need to go faster.",
  "homepage": "https://github.com/MaxMilton/new-tab",
  "author": "Max Milton <max@wearegenki.com>",
  "license": "MIT",
  "keywords": [
    "google-chrome",
    "ntp",
    "chrome-extension",
    "chrome",
    "svelte",
    "maxmilton"
  ],
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "APP_RELEASE=$(git describe --always --dirty=\"-dev\") NODE_ENV=production rollup --config",
    "prebuild": "rm -rf dist && mkdir dist && cp icon* dist",
    "lint": "yarn lint:css && yarn lint:js",
    "lint:css": "stylelint **/*.{css,html} --cache --cache-location=/tmp/",
    "lint:js": "eslint --ignore-path .gitignore --ext html,js .",
    "test": "yarn lint && JEST_CIRCUS=1 jest --notify --watch",
    "test-ci": "JEST_CIRCUS=1 jest --coverage --runInBand --ci"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "0.8.2",
    "@minna-ui/eslint-config": "0.16.0",
    "@minna-ui/jest-config": "0.16.0",
    "@minna-ui/stylelint-config": "0.16.0",
    "@minna-ui/svelte-preprocess-markup": "0.16.0",
    "@minna-ui/svelte-preprocess-style": "0.16.0",
    "@types/chrome": "0.0.74",
    "@wearegenki/renovate-config": "1.0.0",
    "autoprefixer": "9.2.1",
    "chokidar": "2.0.4",
    "codecov": "3.1.0",
    "crass": "0.12.3",
    "eslint": "5.7.0",
    "jest": "23.6.0",
    "postcss": "7.0.5",
    "postcss-import": "12.0.0",
    "postcss-nested": "4.1.0",
    "rollup": "0.66.6",
    "rollup-plugin-analyzer": "2.1.0",
    "rollup-plugin-svelte": "4.3.2",
    "sinon-chrome": "2.3.2",
    "stylelint": "9.7.0",
    "svelte": "2.14.1"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
