{
  "name": "new-tab",
  "version": "0.8.0",
  "description": "âš¡ A high performance new tab page that gets you where you need to go faster.",
  "homepage": "https://github.com/MaxMilton/new-tab",
  "author": "Max Milton <max@wearegenki.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "google-chrome",
    "ntp",
    "chrome-extension",
    "chrome",
    "svelte",
    "maxmilton"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "APP_RELEASE=$(git describe --always --dirty=\"-dev\") NODE_ENV=production rollup -c",
    "prebuild": "rm -rf dist && mkdir dist",
    "lint:css": "stylelint --cache --cache-location=/tmp/ \"**/*.{html,css}\"",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.html src test",
    "lint": "yarn lint:css && yarn lint:js",
    "test": "jest",
    "test-ci": "yarn test --coverage --runInBand --ci",
    "pretest:dev": "yarn lint",
    "test:dev": "yarn test --notify --watch"
  },
  "dependencies": {
    "@sentry/browser": "4.0.0-beta.12"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "0.6.1",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.56",
    "@minna-ui/eslint-config": "0.10.0",
    "@minna-ui/jest-config": "0.10.0",
    "@minna-ui/stylelint-config": "0.10.0",
    "@minna-ui/svelte-preprocess-markup": "0.10.0",
    "@minna-ui/svelte-preprocess-style": "0.10.0",
    "@types/chrome": "0.0.71",
    "autoprefixer": "9.1.0",
    "babel-eslint": "8.2.6",
    "codecov": "3.0.4",
    "crass": "0.12.3",
    "eslint": "5.3.0",
    "jest": "23.4.2",
    "postcss": "7.0.2",
    "postcss-import": "12.0.0",
    "postcss-nested": "3.0.0",
    "rollup": "0.63.5",
    "rollup-plugin-commonjs": "9.1.4",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-svelte": "4.2.1",
    "rollup-plugin-terser": "1.0.1",
    "sinon-chrome": "2.3.2",
    "stylelint": "9.4.0",
    "svelte": "2.9.11"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
